{
  "name": "bettercode-desktop",
  "version": "0.0.19",
  "private": true,
  "description": "BetterCode - UI for parallel work with AI agents",
  "author": "BetterCode",
  "main": ".vite/build/index.js",
  "scripts": {
    "dev": "electron-forge start",
    "build": "electron-forge package",
    "package": "electron-forge make",
    "package:mac": "electron-forge make --platform darwin",
    "package:win": "electron-forge make --platform win32",
    "package:linux": "electron-forge make --platform linux",
    "publish": "electron-forge publish",
    "dist:manifest": "node scripts/generate-update-manifest.mjs",
    "dist:upload": "node scripts/upload-release.mjs",
    "claude:download": "node scripts/download-claude-binary.mjs",
    "claude:download:all": "node scripts/download-claude-binary.mjs --all",
    "release": "rm -rf out && bun run claude:download && bun run package:mac && bun run dist:manifest && ./scripts/upload-release-wrangler.sh",
    "sync:public": "./scripts/sync-to-public.sh",
    "icon:generate": "node scripts/generate-icon.mjs",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "prepare": "husky"
  },
  "dependencies": {
